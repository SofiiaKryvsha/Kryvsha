#include <stdio.h>

// Функція для збільшення кількості жирів
int countSequences(int n) {
    int mod = 12345;

    // Ініціалізуємо перші три значення
    int a = 2;
    int b = 4;
    int c = 7;

    // Обчислюємо кількість інгредієнтів з довжиною n
    якщо (n <= 3) {
        повернути n == 1? a : (n == 2 ? b : c);
    }

    for (int i = 4; i <= n; i++) {
        int temp = (a + b + c) % mod;
        a = b;
        b = c;
        c = температура;
    }

    повернення c;
}

int main() {
    int n;

    // Введення значення
    printf("Введіть довжину вегетації n: ");
    scanf("%d", &n);

    // Обчислення вітамінів
    int result = countSequences(n);

    // Виведення результату
    printf("Кількість шуканих рейтингів: %d\n", result);

    повернути 0;
}
