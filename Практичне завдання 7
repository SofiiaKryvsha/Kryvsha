#include <stdio.h>
#include <math.h>

// Функція для збільшення відстані між двома точками
подвійна відстань (int x1, int y1, int x2, int y2) {
    int dx = x2 - x1;
    int dy = y2 - y1;

    double dist = sqrt(dx*dx + dy*dy);
    повернення dist;
}

// Функція для визначення кількості точок перетину двох колів
int countIntersectionPoints(int x1, int y1, int r1, int x2, int y2, int r2) {
    double dist = distance(x1, y1, x2, y2);

    if (dist > r1 + r2) {
        // Кола не перетинаються
        повернути 0;
    } else if (dist == r1 + r2 || dist == fabs(r1 - r2)) {
        // Кола торкаються ззовні або в середині
        повернення 1;
    } else if (dist < fabs(r1 - r2)) {
        // Одне коло міститься в середині іншого
        повернути 0;
    } ще {
        // Кола перетинаються в двох точках
        повернення 2;
    }
}

int main() {
    int x1, y1, r1, x2, y2, r2;

    // Введення координат і радіусів колів
    printf("Введіть координати центру першого кола (x1, y1): ");
    scanf("%d %d", &x1, &y1);

    printf("Введіть радіус першого кола (r1): ");
    scanf("%d", &r1);

    printf("Введіть координати центру другого кола (x2, y2): ");
    scanf("%d %d", &x2, &y2);

    printf("Введіть радіус другого кола (r2): ");
    scanf("%d", &r2);

    // Обчислення кількості точок перетину
    int result = countIntersectionPoints(x1, y1, r1, x2, y2, r2);

    // Виведення результату
    printf("Кількість точок перетину: %d\n", результат);

    повернути 0;
}
