#include <stdio.h>

// Функція для підрахунку рівних дільників чисел
int countEqualDivisors(int n) {
    int count = 0;

    // Перебираємо значення дільника m від 1 до n/2
    for (int m = 1; m <= n / 2; m++) {
        // Перевіряємо, чи m є рівним дільником числа n
        якщо (n / m == n % m) {
            рахувати++;
        }
    }

    рахунок повернення;
}

int main() {
    int n;

    // Введення значення
    printf("Введіть натуральне число n: ");
    scanf("%d", &n);

    // Підрахунок кількості рівних дільників
    int result = countEqualDivisors(n);

    // Виведення результату
    printf("Кількість рівних дільників числа %d: %d\n", n, результат);

    повернути 0;
}
