#include <stdio.h>
#include <math.h>

// Функція для обчислення довжини вектора
double vectorLength(int x1, int y1, int x2, int y2) {
    int dx = x2 - x1;
    int dy = y2 - y1;

    подвійна довжина = sqrt(dx*dx + dy*dy);
    довжина повернення;
}

int main() {
    int x1, y1, x2, y2;

    // Введення координат
    printf("Введіть координати початкового вектора (x1, y1): ");
    scanf("%d %d", &x1, &y1);

    printf("Введіть координати кінця вектора (x2, y2): ");
    scanf("%d %d", &x2, &y2);

    // Обчислення довжини вектора
    подвійний результат = vectorLength(x1, y1, x2, y2);

    // Виведення результату
    printf("Довжина вектора: %.6f\n", результат);

    повернути 0;
}
