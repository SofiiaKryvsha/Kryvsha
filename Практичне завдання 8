#include <stdio.h>
#include <string.h>

// Функція для обчислення факторіалу числа
int factorial(int n) {
    якщо (n == 0 || n == 1) {
        повернення 1;
    } ще {
        повернути n * факторіал (n - 1);
    }
}

// Функція для підрахунку кількості анаграм
int countAnagrams(char word[]) {
    int length = strlen(слово);
    int count[26] = {0}; // Масив для підрахунку кожної букви

    // Підрахунок кожної букви у слові
    for (int i = 0; i < length; i++) {
        кількість[слово[i] - 'A']++;
    }

    // Обчислення факторіалу кількість букв та поділ на факторіали повторюваних букв
    int знаменник = 1;
    for (int i = 0; i < 26; i++) {
        if (count[i] > 1) {
            знаменник *= факторіал(count[i]);
        }
    }

    // Обчислення кількості анаграм
    int чисельник = факторіал (довжина);
    int анаграми = чисельник / знаменник;

    зворотні анаграми;
}

int main() {
    char слово [15];

    // Введення слова
    printf("Введіть слово: ");
    scanf("%s", слово);

    // Обчислення кількості анаграм
    int result = countAnagrams(word);

    // Виведення результату
    printf("Кількість анаграм: %d\n", результат);

    повернути 0;
}
